{
  "canisters": {
    "backend": {
      "main": "backend/main.mo",
      "type": "motoko",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "frontend/src/declarations/backend"
      }
    },
    "content": {
      "main": "content/main.mo",
      "type": "motoko",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "frontend/src/declarations/content"
      }
    },
    "socialgraph": {
      "main": "socialgraph/main.mo",
      "type": "motoko",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "frontend/src/declarations/socialgraph"
      }
    },
    "messaging": {
      "main": "messaging/main.mo",
      "type": "motoko",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "frontend/src/declarations/messaging"
      }
    },
    "frontend": {
      "dependencies": ["backend", "content", "socialgraph", "messaging"],
      "type": "assets",
      "source": ["frontend/dist"],
      "candid": "frontend/assetstorage.did"
    }
  },
  "networks": {
    "ic": {
      "type": "persistent",
      "providers": ["https://ic0.app"]
    },
    "local": {
      "type": "ephemeral",
      "bind": "127.0.0.1:4943"
    }
  }
}
